        and ol.c_orderline_id = il.c_orderline_id
        and il.c_invoice_id = i.c_invoice_id
        and i.documentno = ?
        and i.c_bpartner_id = o.c_bpartner_id
        and i.c_bpartner_location_id = coalesce(o.billto_id, o.c_bpartner_location_id)
      ]]>
